{
  "name": "mykasbai-monorepo",
  "private": true,
  "scripts": {
    "build": "pnpm -r build",
    "dev": "pnpm -r --parallel dev",
    "start": "pnpm -r start",
    "lint": "pnpm -r lint",
    "test": "pnpm -r test",
    "test:e2e": "pnpm -F apps/api test:e2e",
    "prisma:migrate": "pnpm -F apps/api prisma migrate deploy",
    "prisma:studio": "pnpm -F apps/api prisma studio"
  },
  "devDependencies": {
    "typescript": "^5.3.3",
    "prettier": "^3.2.5",
    "@types/node": "^20.11.20"
  },
  "packageManager": "pnpm@8.15.5"
}
